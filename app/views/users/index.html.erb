<div class="row" style="margin-top: 40px;">
  <table>
    <tr>
      <th>Email</th>
      <th>User id</th>
    </tr>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%=  user.email %>
      </td>
      <td>
        <%=  user.id %>
      </td>
    </tr>
  <% end %>
  </table>

  <%=  link_to "new user", new_user_path %>
</div>